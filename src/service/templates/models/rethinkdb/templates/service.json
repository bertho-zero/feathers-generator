{
  "require": "./{{options.name}}.model",
  "options": [{
    "db": "config.{{answers.model.template}}.db",
    "table":"{{options.name}}",
    "paginate": "config.paginate"
  }],
  "before":{
    "all": [{ "require": "./hooks/before", "options": [] }]
  },
  "after":{
    "all": [{ "require": "./hooks/after", "options": [] }]
  },
  "filter": {
    "all": {
      "require": "./{{options.name}}.filter",
      "options": []
    }
  }
}
